#include <iostream>
#include <string>
#include <algorithm>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t>0)
    {
        string a,b;
        cin>>a>>b;
        sort(a.begin(),a.end());
        sort(b.begin(),b.end());
        if((a.compare(b))==0)
        cout<<"YES"<<endl;
        else
        cout<<"NO"<<endl;
        t--;
    }
    return 0;
}
